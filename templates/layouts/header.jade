.container.header-width.no-padding
  .page-header.row
    .col-lg-3.col-md-3.col-sm-3.col-xs-10.col-nav
      include header-navigation.jade
    .col-lg-6.col-md-6.col-sm-6.col-xs-1.col-callout
      if header.showCallout
        include header-callout.jade
    .col-lg-3.col-md-3.col-sm-3.hidden-xs.col-social
      if header.showSocial
        include header-social.jade